<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite de Contrato">
    <test verbose="2" preserve-order="true" name="Teste de Contrato">
        <classes>
            <class name="com.vemser.rest.tests.login.LoginTest">
                <methods>
                    <include name="testValidarSchemaLoginComSucesso"/>
                </methods>
            </class>
            <class name="com.vemser.rest.tests.produtos.BuscarProdutoTest">
                <methods>
                    <include name="testValidarSchemaListarProdutosComSucesso"/>
                    <include name="testValidarSchemaBuscarPorIdComSucesso"/>
                </methods>
            </class>
            <class name="com.vemser.rest.tests.usuarios.AtualizarUsuariosTest">
                <methods>
                    <include name="testValidarSchemaAtualizarUsuarioComSucesso"/>
                </methods>
            </class>
