                <methods>
                    <include name="testValidarSchemaListarTodosUsuariosSemFiltroComSucesso"/>
                    <include name="testValidarSchemaBuscarUsuariosPorIDValidoComSucesso"/>
                </methods>
            </class>
            <class name="com.vemser.rest.tests.usuarios.CadastrarUsuariosTest">
                <methods>
                    <include name="testValidarSchemaCadastroComSucesso"/>
                </methods>
            </class>
            <class name="com.vemser.rest.tests.usuarios.DeleteUsuariosTest">
                <methods>
                    <include name="testValidarSchemaDeletarUsuarioComSucesso"/>
                </methods>
            </class>
